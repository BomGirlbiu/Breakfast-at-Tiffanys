{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.some.js\";\nimport axios from 'axios';\nconst API_BASE_URL = 'http://localhost:5050/api';\nexport default {\n  name: 'BreadInventory',\n  data() {\n    return {\n      searchTerm: '',\n      view: 'grid',\n      selectedCategory: 'all',\n      categories: [{\n        id: 'french',\n        name: 'Ê≥ïÂºèÈù¢ÂåÖ'\n      }, {\n        id: 'whole-wheat',\n        name: 'ÂÖ®È∫¶Èù¢ÂåÖ'\n      }, {\n        id: 'specialty',\n        name: 'ÁâπËâ≤Èù¢ÂåÖ'\n      }, {\n        id: 'sweet',\n        name: 'ÁîúÈù¢ÂåÖ'\n      }],\n      breadData: [{\n        id: 1,\n        name: 'Ê≥ïÂºèÈïøÊ£çÈù¢ÂåÖ',\n        price: 15.99,\n        stock: 24,\n        inStock: true,\n        image: 'https://via.placeholder.com/80',\n        categoryId: 'french'\n      }, {\n        id: 2,\n        name: 'Ê≥ïÂºè‰π°ÊùëÈù¢ÂåÖ',\n        price: 18.50,\n        stock: 12,\n        inStock: true,\n        image: 'https://via.placeholder.com/80',\n        categoryId: 'french'\n      }, {\n        id: 3,\n        name: 'ÂÖ®È∫¶Èù¢ÂåÖ',\n        price: 12.5,\n        stock: 18,\n        inStock: true,\n        image: 'https://via.placeholder.com/80',\n        categoryId: 'whole-wheat'\n      }, {\n        id: 4,\n        name: 'ÂÖ®È∫¶Ê†∏Ê°ÉÈù¢ÂåÖ',\n        price: 16.99,\n        stock: 8,\n        inStock: true,\n        image: 'https://via.placeholder.com/80',\n        categoryId: 'whole-wheat'\n      }, {\n        id: 5,\n        name: 'ËäùÈ∫ªÈù¢ÂåÖ',\n        price: 9.99,\n        stock: 0,\n        inStock: false,\n        image: 'https://via.placeholder.com/80',\n        categoryId: 'specialty'\n      }, {\n        id: 6,\n        name: 'Ê†∏Ê°ÉÈù¢ÂåÖ',\n        price: 18.5,\n        stock: 12,\n        inStock: true,\n        image: 'https://via.placeholder.com/80',\n        categoryId: 'specialty'\n      }, {\n        id: 7,\n        name: 'Ëë°ËêÑÂπ≤Èù¢ÂåÖ',\n        price: 14.99,\n        stock: 8,\n        inStock: true,\n        image: 'https://via.placeholder.com/80',\n        categoryId: 'sweet'\n      }, {\n        id: 8,\n        name: 'ÈªëÈ∫¶Èù¢ÂåÖ',\n        price: 16.5,\n        stock: 0,\n        inStock: false,\n        image: 'https://via.placeholder.com/80',\n        categoryId: 'specialty'\n      }, {\n        id: 9,\n        name: 'Â•∂Ê≤πÈù¢ÂåÖ',\n        price: 11.99,\n        stock: 15,\n        inStock: true,\n        image: 'https://via.placeholder.com/80',\n        categoryId: 'sweet'\n      }, {\n        id: 10,\n        name: 'ËîìË∂äËéìÈù¢ÂåÖ',\n        price: 19.99,\n        stock: 6,\n        inStock: true,\n        image: 'https://via.placeholder.com/80',\n        categoryId: 'sweet'\n      }],\n      showStockModal: false,\n      stockBread: null,\n      newStock: 0,\n      newInStock: true\n    };\n  },\n  computed: {\n    // Ê†πÊçÆÊêúÁ¥¢ËØçËøáÊª§Èù¢ÂåÖ\n    filteredBread() {\n      return this.breadData.filter(bread => bread.name.toLowerCase().includes(this.searchTerm.toLowerCase()) && (this.selectedCategory === 'all' || bread.categoryId === this.selectedCategory));\n    },\n    // Ëé∑ÂèñËøáÊª§ÂêéÁöÑÂàÜÁ±ªÔºàÂè™ÊòæÁ§∫ÊúâÈù¢ÂåÖÁöÑÂàÜÁ±ªÔºâ\n    filteredCategories() {\n      if (this.selectedCategory === 'all') {\n        // Â¶ÇÊûúÈÄâÊã©ÂÖ®ÈÉ®ÔºåÂè™ËøîÂõûÊúâÂåπÈÖçÈù¢ÂåÖÁöÑÂàÜÁ±ª\n        return this.categories.filter(category => this.breadData.some(bread => bread.categoryId === category.id && bread.name.toLowerCase().includes(this.searchTerm.toLowerCase())));\n      } else {\n        // Â¶ÇÊûúÈÄâÊã©ÁâπÂÆöÂàÜÁ±ªÔºåÂè™ËøîÂõûËØ•ÂàÜÁ±ªÔºàÂ¶ÇÊûúÊúâÂåπÈÖçÁöÑÈù¢ÂåÖÔºâ\n        return this.categories.filter(category => category.id === this.selectedCategory && this.breadData.some(bread => bread.categoryId === category.id && bread.name.toLowerCase().includes(this.searchTerm.toLowerCase())));\n      }\n    },\n    // ËÆ°ÁÆóËøáÊª§ÂêéÁöÑÈù¢ÂåÖÊÄªÊï∞\n    totalFilteredBread() {\n      return this.filteredBread.length;\n    }\n  },\n  methods: {\n    // Ê†πÊçÆÂàÜÁ±ªËé∑ÂèñÈù¢ÂåÖ\n    getBreadByCategory(categoryId) {\n      return this.breadData.filter(bread => bread.categoryId === categoryId && bread.name.toLowerCase().includes(this.searchTerm.toLowerCase()));\n    },\n    openStockModal(bread) {\n      this.stockBread = {\n        ...bread\n      };\n      this.newStock = bread.stock;\n      this.newInStock = bread.inStock;\n      this.showStockModal = true;\n    },\n    async updateStock() {\n      try {\n        const stockData = {\n          stock: this.newStock,\n          inStock: this.newInStock\n        };\n        const response = await axios.put(`${API_BASE_URL}/breads/${this.stockBread.id}/stock`, stockData);\n\n        // Êõ¥Êñ∞Êú¨Âú∞Êï∞ÊçÆ\n        const index = this.breadData.findIndex(b => b.id === this.stockBread.id);\n        if (index !== -1) {\n          this.breadData[index] = {\n            ...this.breadData[index],\n            stock: this.newStock,\n            inStock: this.newInStock\n          };\n        }\n        this.showStockModal = false;\n        this.stockBread = null;\n        alert('Â∫ìÂ≠òÊõ¥Êñ∞ÊàêÂäü');\n      } catch (error) {\n        console.error('Â∫ìÂ≠òÊõ¥Êñ∞Â§±Ë¥•:', error);\n        alert(error.response?.data?.message || 'Â∫ìÂ≠òÊõ¥Êñ∞Â§±Ë¥•ÔºåËØ∑ÈáçËØï');\n      }\n    },\n    closeStockModal() {\n      this.showStockModal = false;\n      this.stockBread = null;\n    }\n  }\n};","map":{"version":3,"names":["axios","API_BASE_URL","name","data","searchTerm","view","selectedCategory","categories","id","breadData","price","stock","inStock","image","categoryId","showStockModal","stockBread","newStock","newInStock","computed","filteredBread","filter","bread","toLowerCase","includes","filteredCategories","category","some","totalFilteredBread","length","methods","getBreadByCategory","openStockModal","updateStock","stockData","response","put","index","findIndex","b","alert","error","console","message","closeStockModal"],"sources":["src/components/BreadInventory.vue"],"sourcesContent":["<template>\n  <div class=\"bread-inventory\">\n    <h1 class=\"page-title\">Èù¢ÂåÖÂ∫ìÂ≠òÁÆ°ÁêÜ</h1>\n    \n    <!-- ÊêúÁ¥¢ÂíåËßÜÂõæÂàáÊç¢ -->\n    <div class=\"control-panel\">\n      <div class=\"search-container\">\n        <input \n          type=\"search\" \n          v-model=\"searchTerm\" \n          placeholder=\"ÊêúÁ¥¢Èù¢ÂåÖ...\" \n          class=\"search-input\"\n        >\n        <i class=\"search-icon\">üîç</i>\n      </div>\n      \n      <div class=\"view-controls\">\n        <button \n          :class=\"['view-btn', { active: view === 'grid' }]\" \n          @click=\"view = 'grid'\"\n        >\n          ÁΩëÊ†ºËßÜÂõæ\n        </button>\n        <button \n          :class=\"['view-btn', { active: view === 'table' }]\" \n          @click=\"view = 'table'\"\n        >\n          Ë°®Ê†ºËßÜÂõæ\n        </button>\n      </div>\n    </div>\n    \n    <!-- ÂàÜÁ±ªÈÄâÊã©Âô® -->\n    <div class=\"category-selector\">\n      <button \n        :class=\"['category-btn', { active: selectedCategory === 'all' }]\" \n        @click=\"selectedCategory = 'all'\"\n      >\n        ÂÖ®ÈÉ®ÂàÜÁ±ª\n      </button>\n      <button \n        v-for=\"category in categories\" \n        :key=\"category.id\"\n        :class=\"['category-btn', { active: selectedCategory === category.id }]\" \n        @click=\"selectedCategory = category.id\"\n      >\n        {{ category.name }}\n      </button>\n    </div>\n    \n    <!-- ÁΩëÊ†ºËßÜÂõæ -->\n    <div v-if=\"view === 'grid'\" class=\"grid-view\">\n      <div v-for=\"category in filteredCategories\" :key=\"category.id\" class=\"category-section\">\n        <h2 class=\"category-title\">{{ category.name }}</h2>\n        <div class=\"bread-grid\">\n          <div \n            v-for=\"bread in getBreadByCategory(category.id)\" \n            :key=\"bread.id\" \n            class=\"bread-card\"\n          >\n            <div class=\"bread-card-header\">\n              <h3 class=\"bread-name\">{{ bread.name }}</h3>\n              <span :class=\"['stock-badge', bread.inStock ? 'in-stock' : 'out-of-stock']\">\n                {{ bread.inStock ? 'ÊúâË¥ß' : 'Áº∫Ë¥ß' }}\n              </span>\n            </div>\n            <div class=\"bread-card-content\">\n              <div class=\"bread-image-container\">\n                <img :src=\"bread.image\" :alt=\"bread.name\" class=\"bread-image\">\n              </div>\n              <div class=\"bread-details\">\n                <div class=\"bread-stock\">\n                  Â∫ìÂ≠ò: <span class=\"stock-value\">{{ bread.stock }}</span>\n                </div>\n                <div class=\"bread-price\">¬•{{ bread.price.toFixed(2) }}</div>\n              </div>\n              <button \n                class=\"update-btn\" \n                :disabled=\"!bread.inStock\"\n                @click=\"openStockModal(bread)\"\n              >\n                {{ bread.inStock ? 'Êõ¥Êñ∞Â∫ìÂ≠ò' : 'Ë°•ÂÖÖÂ∫ìÂ≠ò' }}\n              </button>\n            </div>\n          </div>\n        </div>\n      </div>\n      \n      <!-- Êó†ÁªìÊûúÊèêÁ§∫ -->\n      <div v-if=\"filteredCategories.length === 0\" class=\"no-results\">\n        Ê≤°ÊúâÊâæÂà∞ÂåπÈÖçÁöÑÈù¢ÂåÖ‰∫ßÂìÅ\n      </div>\n    </div>\n    \n    <!-- Ë°®Ê†ºËßÜÂõæ -->\n    <div v-else class=\"table-view\">\n      <div v-for=\"category in filteredCategories\" :key=\"category.id\" class=\"category-section\">\n        <h2 class=\"category-title\">{{ category.name }}</h2>\n        <table class=\"bread-table\">\n          <thead>\n            <tr>\n              <th class=\"image-col\">ÂõæÁâá</th>\n              <th>ÂêçÁß∞</th>\n              <th>‰ª∑Ê†º</th>\n              <th>Â∫ìÂ≠ò</th>\n              <th>Áä∂ÊÄÅ</th>\n              <th class=\"action-col\">Êìç‰Ωú</th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr v-for=\"bread in getBreadByCategory(category.id)\" :key=\"bread.id\">\n              <td>\n                <img :src=\"bread.image\" :alt=\"bread.name\" class=\"table-bread-image\">\n              </td>\n              <td class=\"bread-name\">{{ bread.name }}</td>\n              <td>¬•{{ bread.price.toFixed(2) }}</td>\n              <td>{{ bread.stock }}</td>\n              <td>\n                <span :class=\"['stock-badge', bread.inStock ? 'in-stock' : 'out-of-stock']\">\n                  {{ bread.inStock ? 'ÊúâË¥ß' : 'Áº∫Ë¥ß' }}\n                </span>\n              </td>\n              <td class=\"action-col\">\n                <button \n                  class=\"table-action-btn\" \n                  :disabled=\"!bread.inStock\"\n                  @click=\"openStockModal(bread)\"\n                >\n                  {{ bread.inStock ? 'Êõ¥Êñ∞' : 'Ë°•ÂÖÖ' }}\n                </button>\n              </td>\n            </tr>\n          </tbody>\n        </table>\n      </div>\n      \n      <!-- Êó†ÁªìÊûúÊèêÁ§∫ -->\n      <div v-if=\"filteredCategories.length === 0\" class=\"no-results\">\n        Ê≤°ÊúâÊâæÂà∞ÂåπÈÖçÁöÑÈù¢ÂåÖ‰∫ßÂìÅ\n      </div>\n    </div>\n    \n    <!-- ÂàÜÈ°µ -->\n    <div class=\"pagination\">\n      <div class=\"pagination-info\">\n        ÊòæÁ§∫ {{ totalFilteredBread }} ‰∏™Èù¢ÂåÖ‰∏≠ÁöÑ {{ totalFilteredBread }} ‰∏™\n      </div>\n      <div class=\"pagination-controls\">\n        <button class=\"pagination-btn\" disabled>\n          &lt;\n        </button>\n        <span class=\"pagination-text\">Á¨¨ 1 È°µÔºåÂÖ± 1 È°µ</span>\n        <button class=\"pagination-btn\" disabled>\n          &gt;\n        </button>\n      </div>\n    </div>\n\n    <!-- ÂºπÁ™óÈÉ®ÂàÜ -->\n    <div v-if=\"showStockModal\" class=\"modal\">\n      <div class=\"modal-content\">\n        <div class=\"modal-header\">\n          <h2>Êõ¥Êñ∞Â∫ìÂ≠ò</h2>\n          <button class=\"close-btn\" @click=\"closeStockModal\">&times;</button>\n        </div>\n        <div class=\"modal-body\">\n          <div class=\"form-group\">\n            <label>Â∫ìÂ≠òÊï∞Èáè</label>\n            <input type=\"number\" v-model=\"newStock\" min=\"0\" />\n          </div>\n          <div class=\"form-group\">\n            <label>ÊòØÂê¶‰∏äÊû∂</label>\n            <select v-model=\"newInStock\">\n              <option :value=\"true\">‰∏äÊû∂ÔºàÊúâË¥ßÔºâ</option>\n              <option :value=\"false\">‰∏ãÊû∂ÔºàÁº∫Ë¥ßÔºâ</option>\n            </select>\n          </div>\n          <div class=\"form-actions\">\n            <button class=\"cancel-btn\" @click=\"closeStockModal\">ÂèñÊ∂à</button>\n            <button class=\"save-btn\" @click=\"updateStock\">‰øùÂ≠ò</button>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nconst API_BASE_URL = 'http://localhost:5050/api';\n\nexport default {\n  name: 'BreadInventory',\n  data() {\n    return {\n      searchTerm: '',\n      view: 'grid',\n      selectedCategory: 'all',\n      categories: [\n        { id: 'french', name: 'Ê≥ïÂºèÈù¢ÂåÖ' },\n        { id: 'whole-wheat', name: 'ÂÖ®È∫¶Èù¢ÂåÖ' },\n        { id: 'specialty', name: 'ÁâπËâ≤Èù¢ÂåÖ' },\n        { id: 'sweet', name: 'ÁîúÈù¢ÂåÖ' }\n      ],\n      breadData: [\n        {\n          id: 1,\n          name: 'Ê≥ïÂºèÈïøÊ£çÈù¢ÂåÖ',\n          price: 15.99,\n          stock: 24,\n          inStock: true,\n          image: 'https://via.placeholder.com/80',\n          categoryId: 'french'\n        },\n        {\n          id: 2,\n          name: 'Ê≥ïÂºè‰π°ÊùëÈù¢ÂåÖ',\n          price: 18.50,\n          stock: 12,\n          inStock: true,\n          image: 'https://via.placeholder.com/80',\n          categoryId: 'french'\n        },\n        {\n          id: 3,\n          name: 'ÂÖ®È∫¶Èù¢ÂåÖ',\n          price: 12.5,\n          stock: 18,\n          inStock: true,\n          image: 'https://via.placeholder.com/80',\n          categoryId: 'whole-wheat'\n        },\n        {\n          id: 4,\n          name: 'ÂÖ®È∫¶Ê†∏Ê°ÉÈù¢ÂåÖ',\n          price: 16.99,\n          stock: 8,\n          inStock: true,\n          image: 'https://via.placeholder.com/80',\n          categoryId: 'whole-wheat'\n        },\n        {\n          id: 5,\n          name: 'ËäùÈ∫ªÈù¢ÂåÖ',\n          price: 9.99,\n          stock: 0,\n          inStock: false,\n          image: 'https://via.placeholder.com/80',\n          categoryId: 'specialty'\n        },\n        {\n          id: 6,\n          name: 'Ê†∏Ê°ÉÈù¢ÂåÖ',\n          price: 18.5,\n          stock: 12,\n          inStock: true,\n          image: 'https://via.placeholder.com/80',\n          categoryId: 'specialty'\n        },\n        {\n          id: 7,\n          name: 'Ëë°ËêÑÂπ≤Èù¢ÂåÖ',\n          price: 14.99,\n          stock: 8,\n          inStock: true,\n          image: 'https://via.placeholder.com/80',\n          categoryId: 'sweet'\n        },\n        {\n          id: 8,\n          name: 'ÈªëÈ∫¶Èù¢ÂåÖ',\n          price: 16.5,\n          stock: 0,\n          inStock: false,\n          image: 'https://via.placeholder.com/80',\n          categoryId: 'specialty'\n        },\n        {\n          id: 9,\n          name: 'Â•∂Ê≤πÈù¢ÂåÖ',\n          price: 11.99,\n          stock: 15,\n          inStock: true,\n          image: 'https://via.placeholder.com/80',\n          categoryId: 'sweet'\n        },\n        {\n          id: 10,\n          name: 'ËîìË∂äËéìÈù¢ÂåÖ',\n          price: 19.99,\n          stock: 6,\n          inStock: true,\n          image: 'https://via.placeholder.com/80',\n          categoryId: 'sweet'\n        }\n      ],\n      showStockModal: false,\n      stockBread: null,\n      newStock: 0,\n      newInStock: true\n    }\n  },\n  computed: {\n    // Ê†πÊçÆÊêúÁ¥¢ËØçËøáÊª§Èù¢ÂåÖ\n    filteredBread() {\n      return this.breadData.filter(bread => \n        bread.name.toLowerCase().includes(this.searchTerm.toLowerCase()) &&\n        (this.selectedCategory === 'all' || bread.categoryId === this.selectedCategory)\n      );\n    },\n    \n    // Ëé∑ÂèñËøáÊª§ÂêéÁöÑÂàÜÁ±ªÔºàÂè™ÊòæÁ§∫ÊúâÈù¢ÂåÖÁöÑÂàÜÁ±ªÔºâ\n    filteredCategories() {\n      if (this.selectedCategory === 'all') {\n        // Â¶ÇÊûúÈÄâÊã©ÂÖ®ÈÉ®ÔºåÂè™ËøîÂõûÊúâÂåπÈÖçÈù¢ÂåÖÁöÑÂàÜÁ±ª\n        return this.categories.filter(category => \n          this.breadData.some(bread => \n            bread.categoryId === category.id && \n            bread.name.toLowerCase().includes(this.searchTerm.toLowerCase())\n          )\n        );\n      } else {\n        // Â¶ÇÊûúÈÄâÊã©ÁâπÂÆöÂàÜÁ±ªÔºåÂè™ËøîÂõûËØ•ÂàÜÁ±ªÔºàÂ¶ÇÊûúÊúâÂåπÈÖçÁöÑÈù¢ÂåÖÔºâ\n        return this.categories.filter(category => \n          category.id === this.selectedCategory &&\n          this.breadData.some(bread => \n            bread.categoryId === category.id && \n            bread.name.toLowerCase().includes(this.searchTerm.toLowerCase())\n          )\n        );\n      }\n    },\n    \n    // ËÆ°ÁÆóËøáÊª§ÂêéÁöÑÈù¢ÂåÖÊÄªÊï∞\n    totalFilteredBread() {\n      return this.filteredBread.length;\n    }\n  },\n  methods: {\n    // Ê†πÊçÆÂàÜÁ±ªËé∑ÂèñÈù¢ÂåÖ\n    getBreadByCategory(categoryId) {\n      return this.breadData.filter(bread => \n        bread.categoryId === categoryId &&\n        bread.name.toLowerCase().includes(this.searchTerm.toLowerCase())\n      );\n    },\n    openStockModal(bread) {\n      this.stockBread = { ...bread };\n      this.newStock = bread.stock;\n      this.newInStock = bread.inStock;\n      this.showStockModal = true;\n    },\n    async updateStock() {\n      try {\n        const stockData = {\n          stock: this.newStock,\n          inStock: this.newInStock\n        };\n        \n        const response = await axios.put(\n          `${API_BASE_URL}/breads/${this.stockBread.id}/stock`, \n          stockData\n        );\n        \n        // Êõ¥Êñ∞Êú¨Âú∞Êï∞ÊçÆ\n        const index = this.breadData.findIndex(b => b.id === this.stockBread.id);\n        if (index !== -1) {\n          this.breadData[index] = {\n            ...this.breadData[index],\n            stock: this.newStock,\n            inStock: this.newInStock\n          };\n        }\n        \n        this.showStockModal = false;\n        this.stockBread = null;\n        alert('Â∫ìÂ≠òÊõ¥Êñ∞ÊàêÂäü');\n      } catch (error) {\n        console.error('Â∫ìÂ≠òÊõ¥Êñ∞Â§±Ë¥•:', error);\n        alert(error.response?.data?.message || 'Â∫ìÂ≠òÊõ¥Êñ∞Â§±Ë¥•ÔºåËØ∑ÈáçËØï');\n      }\n    },\n    closeStockModal() {\n      this.showStockModal = false;\n      this.stockBread = null;\n    },\n  }\n}\n</script>\n\n<style scoped>\n.bread-inventory {\n  font-family: 'PingFang SC', 'Helvetica Neue', Arial, sans-serif;\n  max-width: 1200px;\n  margin: 0 auto;\n  padding: 20px;\n  background-color: #f9f9f9;\n}\n\n.page-title {\n  font-size: 24px;\n  font-weight: bold;\n  margin-bottom: 20px;\n  color: #333;\n}\n\n/* ÊéßÂà∂Èù¢ÊùøÊ†∑Âºè */\n.control-panel {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-bottom: 20px;\n  flex-wrap: wrap;\n  gap: 10px;\n}\n\n.search-container {\n  position: relative;\n  width: 300px;\n}\n\n.search-input {\n  width: 100%;\n  padding: 8px 12px 8px 32px;\n  border: 1px solid #ddd;\n  border-radius: 4px;\n  font-size: 14px;\n  transition: border-color 0.3s;\n}\n\n.search-input:focus {\n  outline: none;\n  border-color: #f8b500;\n  box-shadow: 0 0 0 2px rgba(248, 181, 0, 0.2);\n}\n\n.search-icon {\n  position: absolute;\n  left: 10px;\n  top: 50%;\n  transform: translateY(-50%);\n  font-size: 14px;\n  color: #666;\n}\n\n.view-controls {\n  display: flex;\n  gap: 8px;\n}\n\n.view-btn {\n  padding: 8px 12px;\n  background-color: #f0f0f0;\n  border: 1px solid #ddd;\n  border-radius: 4px;\n  cursor: pointer;\n  font-size: 14px;\n  transition: all 0.2s;\n}\n\n.view-btn.active {\n  background-color: #f8b500;\n  color: white;\n  border-color: #f8b500;\n}\n\n/* ÂàÜÁ±ªÈÄâÊã©Âô®Ê†∑Âºè */\n.category-selector {\n  display: flex;\n  gap: 10px;\n  margin-bottom: 20px;\n  flex-wrap: wrap;\n}\n\n.category-btn {\n  padding: 8px 16px;\n  background-color: #f0f0f0;\n  border: 1px solid #ddd;\n  border-radius: 20px