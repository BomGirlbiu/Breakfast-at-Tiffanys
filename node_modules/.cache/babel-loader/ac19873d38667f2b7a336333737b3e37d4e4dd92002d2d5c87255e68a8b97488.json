{"ast":null,"code":"export default {\n  data() {\n    return {\n      sidebarCollapsed: false,\n      activeMenu: '',\n      activePage: {\n        main: '',\n        sub: ''\n      }\n    };\n  },\n  computed: {\n    pageTitle() {\n      if (!this.activePage.main) return '管理系统';\n      const titles = {\n        'inventory': {\n          'list': '库存列表',\n          'analysis': '库存分析'\n        },\n        'finance': {\n          'income': '收入记录',\n          'expense': '支出记录'\n        },\n        'user': {\n          'list': '用户列表',\n          'roles': '用户角色'\n        },\n        'tax': {\n          'records': '税务记录',\n          'reports': '税务报表'\n        },\n        'analysis': {\n          'reports': '财务报表',\n          'forecast': '财务预测'\n        },\n        'settings': '系统设置'\n      };\n      if (this.activePage.main === 'settings') {\n        return titles[this.activePage.main];\n      }\n      return titles[this.activePage.main]?.[this.activePage.sub] || '管理系统';\n    }\n  },\n  methods: {\n    toggleSidebar() {\n      this.sidebarCollapsed = !this.sidebarCollapsed;\n\n      // 如果侧边栏折叠，关闭所有子菜单\n      if (this.sidebarCollapsed) {\n        this.activeMenu = '';\n      }\n    },\n    toggleSubmenu(menu) {\n      if (this.sidebarCollapsed) {\n        // 如果侧边栏折叠，点击菜单项展开侧边栏\n        this.sidebarCollapsed = false;\n        this.activeMenu = menu;\n      } else {\n        // 如果侧边栏已展开，切换子菜单的显示状态\n        this.activeMenu = this.activeMenu === menu ? '' : menu;\n      }\n    },\n    setActivePage(main, sub) {\n      this.activePage = {\n        main,\n        sub\n      };\n\n      // 在移动设备上，选择页面后自动折叠侧边栏\n      if (window.innerWidth < 768) {\n        this.sidebarCollapsed = true;\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","sidebarCollapsed","activeMenu","activePage","main","sub","computed","pageTitle","titles","methods","toggleSidebar","toggleSubmenu","menu","setActivePage","window","innerWidth"],"sources":["src/App.vue"],"sourcesContent":["<!-- <template>\n  <div id=\"app\">\n    <router-view />\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'App'\n}\n</script>\n\n<style>\nhtml, body, #app {\n  height: 100%;\n  margin: 0;\n  padding: 0;\n}\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  height: 100%;\n}\n</style> -->\n\n<template>\n  <div class=\"flex h-screen bg-gray-100\">\n    <!-- Sidebar -->\n    <div \n      class=\"transition-all duration-300 bg-gray-800 text-white\" \n      :class=\"{ 'w-64': !sidebarCollapsed, 'w-20': sidebarCollapsed }\"\n    >\n      <!-- Sidebar Header -->\n      <div class=\"flex items-center justify-between h-16 px-4 border-b border-gray-700\">\n        <h1 class=\"text-xl font-bold\" v-if=\"!sidebarCollapsed\">管理系统</h1>\n        <div class=\"flex justify-center w-full\" v-else>\n          <span class=\"text-xl font-bold\">系统</span>\n        </div>\n        <button @click=\"toggleSidebar\" class=\"p-1 rounded hover:bg-gray-700\">\n          <svg xmlns=\"http://www.w3.org/2000/svg\" class=\"w-6 h-6\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\n            <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M4 6h16M4 12h16M4 18h16\" />\n          </svg>\n        </button>\n      </div>\n      \n      <!-- Sidebar Navigation -->\n      <nav class=\"mt-4\">\n        <ul>\n          <!-- Inventory Management -->\n          <li class=\"mb-2\">\n            <div \n              @click=\"toggleSubmenu('inventory')\" \n              class=\"flex items-center px-4 py-2 cursor-pointer hover:bg-gray-700\"\n              :class=\"{ 'bg-gray-700': activeMenu === 'inventory' }\"\n            >\n              <svg xmlns=\"http://www.w3.org/2000/svg\" class=\"w-6 h-6 mr-3\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\n                <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M20 7l-8-4-8 4m16 0l-8 4m8-4v10l-8 4m0-10L4 7m8 4v10\" />\n              </svg>\n              <span v-if=\"!sidebarCollapsed\">库存管理</span>\n            </div>\n            <ul v-if=\"activeMenu === 'inventory' && !sidebarCollapsed\" class=\"pl-12 mt-2\">\n              <li \n                @click=\"setActivePage('inventory', 'list')\" \n                class=\"py-2 cursor-pointer hover:text-gray-300\"\n                :class=\"{ 'text-blue-400': activePage.main === 'inventory' && activePage.sub === 'list' }\"\n              >\n                库存列表\n              </li>\n              <li \n                @click=\"setActivePage('inventory', 'analysis')\" \n                class=\"py-2 cursor-pointer hover:text-gray-300\"\n                :class=\"{ 'text-blue-400': activePage.main === 'inventory' && activePage.sub === 'analysis' }\"\n              >\n                库存分析\n              </li>\n            </ul>\n          </li>\n          \n          <!-- Income/Expense Management -->\n          <li class=\"mb-2\">\n            <div \n              @click=\"toggleSubmenu('finance')\" \n              class=\"flex items-center px-4 py-2 cursor-pointer hover:bg-gray-700\"\n              :class=\"{ 'bg-gray-700': activeMenu === 'finance' }\"\n            >\n              <svg xmlns=\"http://www.w3.org/2000/svg\" class=\"w-6 h-6 mr-3\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\n                <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M12 8c-1.657 0-3 .895-3 2s1.343 2 3 2 3 .895 3 2-1.343 2-3 2m0-8c1.11 0 2.08.402 2.599 1M12 8V7m0 1v8m0 0v1m0-1c-1.11 0-2.08-.402-2.599-1M21 12a9 9 0 11-18 0 9 9 0 0118 0z\" />\n              </svg>\n              <span v-if=\"!sidebarCollapsed\">收支管理</span>\n            </div>\n            <ul v-if=\"activeMenu === 'finance' && !sidebarCollapsed\" class=\"pl-12 mt-2\">\n              <li \n                @click=\"setActivePage('finance', 'income')\" \n                class=\"py-2 cursor-pointer hover:text-gray-300\"\n                :class=\"{ 'text-blue-400': activePage.main === 'finance' && activePage.sub === 'income' }\"\n              >\n                收入记录\n              </li>\n              <li \n                @click=\"setActivePage('finance', 'expense')\" \n                class=\"py-2 cursor-pointer hover:text-gray-300\"\n                :class=\"{ 'text-blue-400': activePage.main === 'finance' && activePage.sub === 'expense' }\"\n              >\n                支出记录\n              </li>\n            </ul>\n          </li>\n          \n          <!-- User Management -->\n          <li class=\"mb-2\">\n            <div \n              @click=\"toggleSubmenu('user')\" \n              class=\"flex items-center px-4 py-2 cursor-pointer hover:bg-gray-700\"\n              :class=\"{ 'bg-gray-700': activeMenu === 'user' }\"\n            >\n              <svg xmlns=\"http://www.w3.org/2000/svg\" class=\"w-6 h-6 mr-3\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\n                <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M12 4.354a4 4 0 110 5.292M15 21H3v-1a6 6 0 0112 0v1zm0 0h6v-1a6 6 0 00-9-5.197M13 7a4 4 0 11-8 0 4 4 0 018 0z\" />\n              </svg>\n              <span v-if=\"!sidebarCollapsed\">用户管理</span>\n            </div>\n            <ul v-if=\"activeMenu === 'user' && !sidebarCollapsed\" class=\"pl-12 mt-2\">\n              <li \n                @click=\"setActivePage('user', 'list')\" \n                class=\"py-2 cursor-pointer hover:text-gray-300\"\n                :class=\"{ 'text-blue-400': activePage.main === 'user' && activePage.sub === 'list' }\"\n              >\n                用户列表\n              </li>\n              <li \n                @click=\"setActivePage('user', 'roles')\" \n                class=\"py-2 cursor-pointer hover:text-gray-300\"\n                :class=\"{ 'text-blue-400': activePage.main === 'user' && activePage.sub === 'roles' }\"\n              >\n                用户角色\n              </li>\n            </ul>\n          </li>\n          \n          <!-- Tax Management -->\n          <li class=\"mb-2\">\n            <div \n              @click=\"toggleSubmenu('tax')\" \n              class=\"flex items-center px-4 py-2 cursor-pointer hover:bg-gray-700\"\n              :class=\"{ 'bg-gray-700': activeMenu === 'tax' }\"\n            >\n              <svg xmlns=\"http://www.w3.org/2000/svg\" class=\"w-6 h-6 mr-3\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\n                <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M9 14l6-6m-5.5.5h.01m4.99 5h.01M19 21V5a2 2 0 00-2-2H7a2 2 0 00-2 2v16l3.5-2 3.5 2 3.5-2 3.5 2z\" />\n              </svg>\n              <span v-if=\"!sidebarCollapsed\">税务管理</span>\n            </div>\n            <ul v-if=\"activeMenu === 'tax' && !sidebarCollapsed\" class=\"pl-12 mt-2\">\n              <li \n                @click=\"setActivePage('tax', 'records')\" \n                class=\"py-2 cursor-pointer hover:text-gray-300\"\n                :class=\"{ 'text-blue-400': activePage.main === 'tax' && activePage.sub === 'records' }\"\n              >\n                税务记录\n              </li>\n              <li \n                @click=\"setActivePage('tax', 'reports')\" \n                class=\"py-2 cursor-pointer hover:text-gray-300\"\n                :class=\"{ 'text-blue-400': activePage.main === 'tax' && activePage.sub === 'reports' }\"\n              >\n                税务报表\n              </li>\n            </ul>\n          </li>\n          \n          <!-- Financial Analysis -->\n          <li class=\"mb-2\">\n            <div \n              @click=\"toggleSubmenu('analysis')\" \n              class=\"flex items-center px-4 py-2 cursor-pointer hover:bg-gray-700\"\n              :class=\"{ 'bg-gray-700': activeMenu === 'analysis' }\"\n            >\n              <svg xmlns=\"http://www.w3.org/2000/svg\" class=\"w-6 h-6 mr-3\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\n                <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M7 12l3-3 3 3 4-4M8 21l4-4 4 4M3 4h18M4 4h16v12a1 1 0 01-1 1H5a1 1 0 01-1-1V4z\" />\n              </svg>\n              <span v-if=\"!sidebarCollapsed\">财务分析</span>\n            </div>\n            <ul v-if=\"activeMenu === 'analysis' && !sidebarCollapsed\" class=\"pl-12 mt-2\">\n              <li \n                @click=\"setActivePage('analysis', 'reports')\" \n                class=\"py-2 cursor-pointer hover:text-gray-300\"\n                :class=\"{ 'text-blue-400': activePage.main === 'analysis' && activePage.sub === 'reports' }\"\n              >\n                财务报表\n              </li>\n              <li \n                @click=\"setActivePage('analysis', 'forecast')\" \n                class=\"py-2 cursor-pointer hover:text-gray-300\"\n                :class=\"{ 'text-blue-400': activePage.main === 'analysis' && activePage.sub === 'forecast' }\"\n              >\n                财务预测\n              </li>\n            </ul>\n          </li>\n          \n          <!-- System Settings -->\n          <li class=\"mb-2\">\n            <div \n              @click=\"setActivePage('settings', '')\" \n              class=\"flex items-center px-4 py-2 cursor-pointer hover:bg-gray-700\"\n              :class=\"{ 'bg-gray-700': activeMenu === 'settings' }\"\n            >\n              <svg xmlns=\"http://www.w3.org/2000/svg\" class=\"w-6 h-6 mr-3\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\n                <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M10.325 4.317c.426-1.756 2.924-1.756 3.35 0a1.724 1.724 0 002.573 1.066c1.543-.94 3.31.826 2.37 2.37a1.724 1.724 0 001.065 2.572c1.756.426 1.756 2.924 0 3.35a1.724 1.724 0 00-1.066 2.573c.94 1.543-.826 3.31-2.37 2.37a1.724 1.724 0 00-2.572 1.065c-.426 1.756-2.924 1.756-3.35 0a1.724 1.724 0 00-2.573-1.066c-1.543.94-3.31-.826-2.37-2.37a1.724 1.724 0 00-1.065-2.572c-1.756-.426-1.756-2.924 0-3.35a1.724 1.724 0 001.066-2.573c-.94-1.543.826-3.31 2.37-2.37.996.608 2.296.07 2.572-1.065z\" />\n                <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M15 12a3 3 0 11-6 0 3 3 0 016 0z\" />\n              </svg>\n              <span v-if=\"!sidebarCollapsed\">系统设置</span>\n            </div>\n          </li>\n        </ul>\n      </nav>\n    </div>\n    \n    <!-- Main Content -->\n    <div class=\"flex-1 overflow-auto\">\n      <!-- Header -->\n      <header class=\"flex items-center justify-between h-16 px-6 bg-white border-b\">\n        <div class=\"text-xl font-bold\">{{ pageTitle }}</div>\n        <div class=\"flex items-center space-x-4\">\n          <div class=\"relative\">\n            <svg xmlns=\"http://www.w3.org/2000/svg\" class=\"w-6 h-6 text-gray-600\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\n              <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M15 17h5l-1.405-1.405A2.032 2.032 0 0118 14.158V11a6.002 6.002 0 00-4-5.659V5a2 2 0 10-4 0v.341C7.67 6.165 6 8.388 6 11v3.159c0 .538-.214 1.055-.595 1.436L4 17h5m6 0v1a3 3 0 11-6 0v-1m6 0H9\" />\n            </svg>\n            <span class=\"absolute top-0 right-0 w-2 h-2 bg-red-500 rounded-full\"></span>\n          </div>\n          <div class=\"flex items-center\">\n            <img src=\"https://via.placeholder.com/40\" alt=\"User\" class=\"w-8 h-8 rounded-full\">\n            <span class=\"ml-2\">管理员</span>\n          </div>\n        </div>\n      </header>\n      \n      <!-- Page Content -->\n      <main class=\"p-6\">\n        <div v-if=\"activePage.main === 'inventory' && activePage.sub === 'list'\">\n          <h2 class=\"mb-4 text-2xl font-bold\">库存列表</h2>\n          <div class=\"p-4 bg-white rounded-lg shadow\">\n            <table class=\"w-full\">\n              <thead>\n                <tr class=\"border-b\">\n                  <th class=\"p-2 text-left\">产品名称</th>\n                  <th class=\"p-2 text-left\">SKU</th>\n                  <th class=\"p-2 text-left\">库存数量</th>\n                  <th class=\"p-2 text-left\">单价</th>\n                  <th class=\"p-2 text-left\">状态</th>\n                </tr>\n              </thead>\n              <tbody>\n                <tr class=\"border-b hover:bg-gray-50\">\n                  <td class=\"p-2\">产品A</td>\n                  <td class=\"p-2\">SKU001</td>\n                  <td class=\"p-2\">120</td>\n                  <td class=\"p-2\">¥99.00</td>\n                  <td class=\"p-2\"><span class=\"px-2 py-1 text-xs text-green-800 bg-green-100 rounded-full\">充足</span></td>\n                </tr>\n                <tr class=\"border-b hover:bg-gray-50\">\n                  <td class=\"p-2\">产品B</td>\n                  <td class=\"p-2\">SKU002</td>\n                  <td class=\"p-2\">45</td>\n                  <td class=\"p-2\">¥199.00</td>\n                  <td class=\"p-2\"><span class=\"px-2 py-1 text-xs text-yellow-800 bg-yellow-100 rounded-full\">警告</span></td>\n                </tr>\n                <tr class=\"border-b hover:bg-gray-50\">\n                  <td class=\"p-2\">产品C</td>\n                  <td class=\"p-2\">SKU003</td>\n                  <td class=\"p-2\">0</td>\n                  <td class=\"p-2\">¥299.00</td>\n                  <td class=\"p-2\"><span class=\"px-2 py-1 text-xs text-red-800 bg-red-100 rounded-full\">缺货</span></td>\n                </tr>\n              </tbody>\n            </table>\n          </div>\n        </div>\n        \n        <div v-else-if=\"activePage.main === 'inventory' && activePage.sub === 'analysis'\">\n          <h2 class=\"mb-4 text-2xl font-bold\">库存分析</h2>\n          <div class=\"grid grid-cols-1 gap-6 md:grid-cols-2\">\n            <div class=\"p-4 bg-white rounded-lg shadow\">\n              <h3 class=\"mb-2 text-lg font-semibold\">库存周转率</h3>\n              <div class=\"h-64 bg-gray-100 rounded\"></div>\n            </div>\n            <div class=\"p-4 bg-white rounded-lg shadow\">\n              <h3 class=\"mb-2 text-lg font-semibold\">库存价值分布</h3>\n              <div class=\"h-64 bg-gray-100 rounded\"></div>\n            </div>\n          </div>\n        </div>\n        \n        <div v-else-if=\"activePage.main === 'finance' && activePage.sub === 'income'\">\n          <h2 class=\"mb-4 text-2xl font-bold\">收入记录</h2>\n          <div class=\"p-4 bg-white rounded-lg shadow\">\n            <table class=\"w-full\">\n              <thead>\n                <tr class=\"border-b\">\n                  <th class=\"p-2 text-left\">订单号</th>\n                  <th class=\"p-2 text-left\">客户</th>\n                  <th class=\"p-2 text-left\">金额</th>\n                  <th class=\"p-2 text-left\">日期</th>\n                  <th class=\"p-2 text-left\">状态</th>\n                </tr>\n              </thead>\n              <tbody>\n                <tr class=\"border-b hover:bg-gray-50\">\n                  <td class=\"p-2\">ORD-2023-001</td>\n                  <td class=\"p-2\">张三</td>\n                  <td class=\"p-2\">¥1,299.00</td>\n                  <td class=\"p-2\">2023-05-15</td>\n                  <td class=\"p-2\"><span class=\"px-2 py-1 text-xs text-green-800 bg-green-100 rounded-full\">已完成</span></td>\n                </tr>\n                <tr class=\"border-b hover:bg-gray-50\">\n                  <td class=\"p-2\">ORD-2023-002</td>\n                  <td class=\"p-2\">李四</td>\n                  <td class=\"p-2\">¥899.00</td>\n                  <td class=\"p-2\">2023-05-14</td>\n                  <td class=\"p-2\"><span class=\"px-2 py-1 text-xs text-green-800 bg-green-100 rounded-full\">已完成</span></td>\n                </tr>\n              </tbody>\n            </table>\n          </div>\n        </div>\n        \n        <div v-else-if=\"activePage.main === 'finance' && activePage.sub === 'expense'\">\n          <h2 class=\"mb-4 text-2xl font-bold\">支出记录</h2>\n          <div class=\"p-4 bg-white rounded-lg shadow\">\n            <table class=\"w-full\">\n              <thead>\n                <tr class=\"border-b\">\n                  <th class=\"p-2 text-left\">支出编号</th>\n                  <th class=\"p-2 text-left\">类别</th>\n                  <th class=\"p-2 text-left\">金额</th>\n                  <th class=\"p-2 text-left\">日期</th>\n                  <th class=\"p-2 text-left\">状态</th>\n                </tr>\n              </thead>\n              <tbody>\n                <tr class=\"border-b hover:bg-gray-50\">\n                  <td class=\"p-2\">EXP-2023-001</td>\n                  <td class=\"p-2\">办公用品</td>\n                  <td class=\"p-2\">¥2,500.00</td>\n                  <td class=\"p-2\">2023-05-10</td>\n                  <td class=\"p-2\"><span class=\"px-2 py-1 text-xs text-green-800 bg-green-100 rounded-full\">已支付</span></td>\n                </tr>\n                <tr class=\"border-b hover:bg-gray-50\">\n                  <td class=\"p-2\">EXP-2023-002</td>\n                  <td class=\"p-2\">水电费</td>\n                  <td class=\"p-2\">¥1,200.00</td>\n                  <td class=\"p-2\">2023-05-05</td>\n                  <td class=\"p-2\"><span class=\"px-2 py-1 text-xs text-green-800 bg-green-100 rounded-full\">已支付</span></td>\n                </tr>\n              </tbody>\n            </table>\n          </div>\n        </div>\n        \n        <div v-else-if=\"activePage.main === 'user' && activePage.sub === 'list'\">\n          <h2 class=\"mb-4 text-2xl font-bold\">用户列表</h2>\n          <div class=\"p-4 bg-white rounded-lg shadow\">\n            <table class=\"w-full\">\n              <thead>\n                <tr class=\"border-b\">\n                  <th class=\"p-2 text-left\">用户ID</th>\n                  <th class=\"p-2 text-left\">姓名</th>\n                  <th class=\"p-2 text-left\">邮箱</th>\n                  <th class=\"p-2 text-left\">角色</t