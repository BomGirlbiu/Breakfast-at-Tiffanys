{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport * as echarts from 'echarts';\nexport default {\n  name: 'FinancialPage',\n  data() {\n    return {\n      // 概览数据\n      monthlyIncome: 45678.90,\n      monthlyExpense: 23456.78,\n      monthlyProfit: 22222.12,\n      incomeTrend: 12.5,\n      expenseTrend: -5.2,\n      profitTrend: 8.3,\n      // 日期范围\n      startDate: '2024-01-01',\n      endDate: '2024-12-31',\n      selectedType: 'all',\n      // 图表实例\n      trendChart: null,\n      incomePieChart: null,\n      expensePieChart: null,\n      // 模拟数据\n      transactions: [{\n        id: 1,\n        date: '2024-03-15',\n        type: 'income',\n        category: '面包销售',\n        amount: 12345.67,\n        note: '日常销售'\n      }, {\n        id: 2,\n        date: '2024-03-14',\n        type: 'expense',\n        category: '原料采购',\n        amount: 5678.90,\n        note: '面粉采购'\n      }\n      // 更多交易数据...\n      ]\n    };\n  },\n  computed: {\n    filteredTransactions() {\n      return this.transactions.filter(item => {\n        const dateInRange = item.date >= this.startDate && item.date <= this.endDate;\n        const typeMatch = this.selectedType === 'all' || item.type === this.selectedType;\n        return dateInRange && typeMatch;\n      });\n    }\n  },\n  mounted() {\n    this.initCharts();\n  },\n  methods: {\n    initCharts() {\n      // 初始化趋势图\n      this.trendChart = echarts.init(this.$refs.trendChart);\n      this.trendChart.setOption({\n        title: {\n          text: '近6个月收支趋势'\n        },\n        tooltip: {\n          trigger: 'axis'\n        },\n        legend: {\n          data: ['收入', '支出', '利润']\n        },\n        xAxis: {\n          type: 'category',\n          data: ['10月', '11月', '12月', '1月', '2月', '3月']\n        },\n        yAxis: {\n          type: 'value'\n        },\n        series: [{\n          name: '收入',\n          type: 'line',\n          data: [35000, 38000, 42000, 40000, 43000, 45678]\n        }, {\n          name: '支出',\n          type: 'line',\n          data: [20000, 22000, 21000, 23000, 22500, 23456]\n        }, {\n          name: '利润',\n          type: 'line',\n          data: [15000, 16000, 21000, 17000, 20500, 22222]\n        }]\n      });\n\n      // 初始化收入构成饼图\n      this.incomePieChart = echarts.init(this.$refs.incomePieChart);\n      this.incomePieChart.setOption({\n        title: {\n          text: '收入构成'\n        },\n        tooltip: {\n          trigger: 'item'\n        },\n        legend: {\n          orient: 'vertical',\n          left: 'left'\n        },\n        series: [{\n          type: 'pie',\n          radius: '50%',\n          data: [{\n            value: 23500,\n            name: '面包销售'\n          }, {\n            value: 12000,\n            name: '饮品销售'\n          }, {\n            value: 5000,\n            name: '其他收入'\n          }, {\n            value: 5178,\n            name: '会员充值'\n          }],\n          emphasis: {\n            itemStyle: {\n              shadowBlur: 10,\n              shadowOffsetX: 0,\n              shadowColor: 'rgba(0, 0, 0, 0.5)'\n            }\n          }\n        }]\n      });\n\n      // 初始化支出构成饼图\n      this.expensePieChart = echarts.init(this.$refs.expensePieChart);\n      this.expensePieChart.setOption({\n        title: {\n          text: '支出构成'\n        },\n        tooltip: {\n          trigger: 'item'\n        },\n        legend: {\n          orient: 'vertical',\n          left: 'left'\n        },\n        series: [{\n          type: 'pie',\n          radius: '50%',\n          data: [{\n            value: 12000,\n            name: '原料采购'\n          }, {\n            value: 5000,\n            name: '人工成本'\n          }, {\n            value: 3000,\n            name: '水电费用'\n          }, {\n            value: 2000,\n            name: '其他支出'\n          }, {\n            value: 1456,\n            name: '设备维护'\n          }],\n          emphasis: {\n            itemStyle: {\n              shadowBlur: 10,\n              shadowOffsetX: 0,\n              shadowColor: 'rgba(0, 0, 0, 0.5)'\n            }\n          }\n        }]\n      });\n\n      // 监听窗口大小变化，调整图表大小\n      window.addEventListener('resize', this.handleResize);\n    },\n    handleResize() {\n      this.trendChart?.resize();\n      this.incomePieChart?.resize();\n      this.expensePieChart?.resize();\n    },\n    formatNumber(num) {\n      return num.toLocaleString('zh-CN', {\n        minimumFractionDigits: 2,\n        maximumFractionDigits: 2\n      });\n    },\n    formatDate(date) {\n      return new Date(date).toLocaleDateString('zh-CN');\n    }\n  },\n  beforeDestroy() {\n    window.removeEventListener('resize', this.handleResize);\n    this.trendChart?.dispose();\n    this.incomePieChart?.dispose();\n    this.expensePieChart?.dispose();\n  }\n};","map":{"version":3,"names":["echarts","name","data","monthlyIncome","monthlyExpense","monthlyProfit","incomeTrend","expenseTrend","profitTrend","startDate","endDate","selectedType","trendChart","incomePieChart","expensePieChart","transactions","id","date","type","category","amount","note","computed","filteredTransactions","filter","item","dateInRange","typeMatch","mounted","initCharts","methods","init","$refs","setOption","title","text","tooltip","trigger","legend","xAxis","yAxis","series","orient","left","radius","value","emphasis","itemStyle","shadowBlur","shadowOffsetX","shadowColor","window","addEventListener","handleResize","resize","formatNumber","num","toLocaleString","minimumFractionDigits","maximumFractionDigits","formatDate","Date","toLocaleDateString","beforeDestroy","removeEventListener","dispose"],"sources":["src/components/FinancialPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"financial-page\">\r\n    <h1 class=\"page-title\">财务分析</h1>\r\n    \r\n    <!-- 财务概览卡片 -->\r\n    <div class=\"financial-overview\">\r\n      <div class=\"overview-card income\">\r\n        <h3>本月收入</h3>\r\n        <div class=\"amount\">¥{{ formatNumber(monthlyIncome) }}</div>\r\n        <div class=\"trend\" :class=\"{ 'up': incomeTrend > 0, 'down': incomeTrend < 0 }\">\r\n          {{ incomeTrend > 0 ? '+' : '' }}{{ incomeTrend }}%\r\n        </div>\r\n      </div>\r\n      <div class=\"overview-card expense\">\r\n        <h3>本月支出</h3>\r\n        <div class=\"amount\">¥{{ formatNumber(monthlyExpense) }}</div>\r\n        <div class=\"trend\" :class=\"{ 'up': expenseTrend > 0, 'down': expenseTrend < 0 }\">\r\n          {{ expenseTrend > 0 ? '+' : '' }}{{ expenseTrend }}%\r\n        </div>\r\n      </div>\r\n      <div class=\"overview-card profit\">\r\n        <h3>本月利润</h3>\r\n        <div class=\"amount\">¥{{ formatNumber(monthlyProfit) }}</div>\r\n        <div class=\"trend\" :class=\"{ 'up': profitTrend > 0, 'down': profitTrend < 0 }\">\r\n          {{ profitTrend > 0 ? '+' : '' }}{{ profitTrend }}%\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 图表区域 -->\r\n    <div class=\"charts-container\">\r\n      <!-- 收入支出趋势图 -->\r\n      <div class=\"chart-card\">\r\n        <h3>收入支出趋势</h3>\r\n        <div ref=\"trendChart\" class=\"chart\"></div>\r\n      </div>\r\n      \r\n      <!-- 收入构成饼图 -->\r\n      <div class=\"chart-card\">\r\n        <h3>收入构成</h3>\r\n        <div ref=\"incomePieChart\" class=\"chart\"></div>\r\n      </div>\r\n      \r\n      <!-- 支出构成饼图 -->\r\n      <div class=\"chart-card\">\r\n        <h3>支出构成</h3>\r\n        <div ref=\"expensePieChart\" class=\"chart\"></div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 详细数据表格 -->\r\n    <div class=\"data-table-container\">\r\n      <h3>财务明细</h3>\r\n      <div class=\"table-controls\">\r\n        <div class=\"date-range\">\r\n          <input type=\"date\" v-model=\"startDate\" class=\"date-input\">\r\n          <span>至</span>\r\n          <input type=\"date\" v-model=\"endDate\" class=\"date-input\">\r\n        </div>\r\n        <div class=\"table-filters\">\r\n          <select v-model=\"selectedType\" class=\"filter-select\">\r\n            <option value=\"all\">全部类型</option>\r\n            <option value=\"income\">收入</option>\r\n            <option value=\"expense\">支出</option>\r\n          </select>\r\n        </div>\r\n      </div>\r\n      <table class=\"data-table\">\r\n        <thead>\r\n          <tr>\r\n            <th>日期</th>\r\n            <th>类型</th>\r\n            <th>分类</th>\r\n            <th>金额</th>\r\n            <th>备注</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"item in filteredTransactions\" :key=\"item.id\">\r\n            <td>{{ formatDate(item.date) }}</td>\r\n            <td>\r\n              <span :class=\"['type-badge', item.type]\">\r\n                {{ item.type === 'income' ? '收入' : '支出' }}\r\n              </span>\r\n            </td>\r\n            <td>{{ item.category }}</td>\r\n            <td :class=\"item.type === 'income' ? 'income-amount' : 'expense-amount'\">\r\n              {{ item.type === 'income' ? '+' : '-' }}¥{{ formatNumber(item.amount) }}\r\n            </td>\r\n            <td>{{ item.note }}</td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport * as echarts from 'echarts'\r\n\r\nexport default {\r\n  name: 'FinancialPage',\r\n  data() {\r\n    return {\r\n      // 概览数据\r\n      monthlyIncome: 45678.90,\r\n      monthlyExpense: 23456.78,\r\n      monthlyProfit: 22222.12,\r\n      incomeTrend: 12.5,\r\n      expenseTrend: -5.2,\r\n      profitTrend: 8.3,\r\n\r\n      // 日期范围\r\n      startDate: '2024-01-01',\r\n      endDate: '2024-12-31',\r\n      selectedType: 'all',\r\n\r\n      // 图表实例\r\n      trendChart: null,\r\n      incomePieChart: null,\r\n      expensePieChart: null,\r\n\r\n      // 模拟数据\r\n      transactions: [\r\n        {\r\n          id: 1,\r\n          date: '2024-03-15',\r\n          type: 'income',\r\n          category: '面包销售',\r\n          amount: 12345.67,\r\n          note: '日常销售'\r\n        },\r\n        {\r\n          id: 2,\r\n          date: '2024-03-14',\r\n          type: 'expense',\r\n          category: '原料采购',\r\n          amount: 5678.90,\r\n          note: '面粉采购'\r\n        },\r\n        // 更多交易数据...\r\n      ]\r\n    }\r\n  },\r\n  computed: {\r\n    filteredTransactions() {\r\n      return this.transactions.filter(item => {\r\n        const dateInRange = item.date >= this.startDate && item.date <= this.endDate\r\n        const typeMatch = this.selectedType === 'all' || item.type === this.selectedType\r\n        return dateInRange && typeMatch\r\n      })\r\n    }\r\n  },\r\n  mounted() {\r\n    this.initCharts()\r\n  },\r\n  methods: {\r\n    initCharts() {\r\n      // 初始化趋势图\r\n      this.trendChart = echarts.init(this.$refs.trendChart)\r\n      this.trendChart.setOption({\r\n        title: {\r\n          text: '近6个月收支趋势'\r\n        },\r\n        tooltip: {\r\n          trigger: 'axis'\r\n        },\r\n        legend: {\r\n          data: ['收入', '支出', '利润']\r\n        },\r\n        xAxis: {\r\n          type: 'category',\r\n          data: ['10月', '11月', '12月', '1月', '2月', '3月']\r\n        },\r\n        yAxis: {\r\n          type: 'value'\r\n        },\r\n        series: [\r\n          {\r\n            name: '收入',\r\n            type: 'line',\r\n            data: [35000, 38000, 42000, 40000, 43000, 45678]\r\n          },\r\n          {\r\n            name: '支出',\r\n            type: 'line',\r\n            data: [20000, 22000, 21000, 23000, 22500, 23456]\r\n          },\r\n          {\r\n            name: '利润',\r\n            type: 'line',\r\n            data: [15000, 16000, 21000, 17000, 20500, 22222]\r\n          }\r\n        ]\r\n      })\r\n\r\n      // 初始化收入构成饼图\r\n      this.incomePieChart = echarts.init(this.$refs.incomePieChart)\r\n      this.incomePieChart.setOption({\r\n        title: {\r\n          text: '收入构成'\r\n        },\r\n        tooltip: {\r\n          trigger: 'item'\r\n        },\r\n        legend: {\r\n          orient: 'vertical',\r\n          left: 'left'\r\n        },\r\n        series: [\r\n          {\r\n            type: 'pie',\r\n            radius: '50%',\r\n            data: [\r\n              { value: 23500, name: '面包销售' },\r\n              { value: 12000, name: '饮品销售' },\r\n              { value: 5000, name: '其他收入' },\r\n              { value: 5178, name: '会员充值' }\r\n            ],\r\n            emphasis: {\r\n              itemStyle: {\r\n                shadowBlur: 10,\r\n                shadowOffsetX: 0,\r\n                shadowColor: 'rgba(0, 0, 0, 0.5)'\r\n              }\r\n            }\r\n          }\r\n        ]\r\n      })\r\n\r\n      // 初始化支出构成饼图\r\n      this.expensePieChart = echarts.init(this.$refs.expensePieChart)\r\n      this.expensePieChart.setOption({\r\n        title: {\r\n          text: '支出构成'\r\n        },\r\n        tooltip: {\r\n          trigger: 'item'\r\n        },\r\n        legend: {\r\n          orient: 'vertical',\r\n          left: 'left'\r\n        },\r\n        series: [\r\n          {\r\n            type: 'pie',\r\n            radius: '50%',\r\n            data: [\r\n              { value: 12000, name: '原料采购' },\r\n              { value: 5000, name: '人工成本' },\r\n              { value: 3000, name: '水电费用' },\r\n              { value: 2000, name: '其他支出' },\r\n              { value: 1456, name: '设备维护' }\r\n            ],\r\n            emphasis: {\r\n              itemStyle: {\r\n                shadowBlur: 10,\r\n                shadowOffsetX: 0,\r\n                shadowColor: 'rgba(0, 0, 0, 0.5)'\r\n              }\r\n            }\r\n          }\r\n        ]\r\n      })\r\n\r\n      // 监听窗口大小变化，调整图表大小\r\n      window.addEventListener('resize', this.handleResize)\r\n    },\r\n    handleResize() {\r\n      this.trendChart?.resize()\r\n      this.incomePieChart?.resize()\r\n      this.expensePieChart?.resize()\r\n    },\r\n    formatNumber(num) {\r\n      return num.toLocaleString('zh-CN', {\r\n        minimumFr