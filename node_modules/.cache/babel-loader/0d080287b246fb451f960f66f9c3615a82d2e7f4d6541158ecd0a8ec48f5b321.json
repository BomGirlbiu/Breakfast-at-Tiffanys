{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.some.js\";\nexport default {\n  name: 'BreadInfo',\n  data() {\n    return {\n      searchTerm: '',\n      view: 'grid',\n      selectedCategory: 'all',\n      showEditModal: false,\n      showDeleteModal: false,\n      editingBread: null,\n      breadToDelete: null,\n      ingredientNames: [],\n      ingredientAmounts: [],\n      categories: [{\n        id: 'french',\n        name: 'Ê≥ïÂºèÈù¢ÂåÖ'\n      }, {\n        id: 'whole-wheat',\n        name: 'ÂÖ®È∫¶Èù¢ÂåÖ'\n      }, {\n        id: 'specialty',\n        name: 'ÁâπËâ≤Èù¢ÂåÖ'\n      }, {\n        id: 'sweet',\n        name: 'ÁîúÈù¢ÂåÖ'\n      }],\n      breadData: [{\n        id: 1,\n        name: 'Ê≥ïÂºèÈïøÊ£çÈù¢ÂåÖ',\n        price: 15.99,\n        image: 'https://via.placeholder.com/80',\n        categoryId: 'french',\n        description: '‰º†ÁªüÊ≥ïÂºèÈïøÊ£çÈù¢ÂåÖÔºåÂ§ñÈÖ•ÈáåÂ´©',\n        ingredients: {\n          'È´òÁ≠ãÈù¢Á≤â': '500g',\n          'Ê∞¥': '300ml',\n          'ÈÖµÊØç': '5g',\n          'Áõê': '10g'\n        }\n      }, {\n        id: 2,\n        name: 'Ê≥ïÂºèÁâõËßíÈù¢ÂåÖ',\n        price: 18.50,\n        image: 'https://via.placeholder.com/80',\n        categoryId: 'french',\n        description: 'ÈÖ•ËÑÜÂèØÂè£ÁöÑÊ≥ïÂºèÁâõËßíÈù¢ÂåÖÔºåÂ±ÇÊ¨°ÂàÜÊòé',\n        ingredients: {\n          'È´òÁ≠ãÈù¢Á≤â': '400g',\n          'ÈªÑÊ≤π': '200g',\n          'Á≥ñ': '30g',\n          'Áõê': '8g',\n          'ÈÖµÊØç': '5g',\n          'ÁâõÂ•∂': '150ml'\n        }\n      }, {\n        id: 3,\n        name: 'Ê≥ïÂºè‰π°ÊùëÈù¢ÂåÖ',\n        price: 16.99,\n        image: 'https://via.placeholder.com/80',\n        categoryId: 'french',\n        description: '‰º†ÁªüÊ≥ïÂºè‰π°ÊùëÈù¢ÂåÖÔºåÂè£ÊÑüÊâéÂÆû',\n        ingredients: {\n          'È´òÁ≠ãÈù¢Á≤â': '450g',\n          'ÂÖ®È∫¶Èù¢Á≤â': '50g',\n          'Ê∞¥': '300ml',\n          'ÈÖµÊØç': '5g',\n          'Áõê': '10g'\n        }\n      }, {\n        id: 4,\n        name: 'ÂÖ®È∫¶Èù¢ÂåÖ',\n        price: 12.50,\n        image: 'https://via.placeholder.com/80',\n        categoryId: 'whole-wheat',\n        description: 'ÂÅ•Â∫∑ÂÖ®È∫¶Èù¢ÂåÖÔºåÂØåÂê´ËÜ≥È£üÁ∫§Áª¥',\n        ingredients: {\n          'ÂÖ®È∫¶Èù¢Á≤â': '400g',\n          'È´òÁ≠ãÈù¢Á≤â': '100g',\n          'Ê∞¥': '300ml',\n          'ÈÖµÊØç': '5g',\n          'Áõê': '8g',\n          'Á≥ñ': '15g'\n        }\n      }, {\n        id: 5,\n        name: 'ÂÖ®È∫¶Ê†∏Ê°ÉÈù¢ÂåÖ',\n        price: 16.99,\n        image: 'https://via.placeholder.com/80',\n        categoryId: 'whole-wheat',\n        description: 'Ëê•ÂÖª‰∏∞ÂØåÁöÑÂÖ®È∫¶Ê†∏Ê°ÉÈù¢ÂåÖ',\n        ingredients: {\n          'ÂÖ®È∫¶Èù¢Á≤â': '350g',\n          'È´òÁ≠ãÈù¢Á≤â': '150g',\n          'Ê†∏Ê°É‰ªÅ': '100g',\n          'Ê∞¥': '300ml',\n          'ÈÖµÊØç': '5g',\n          'Áõê': '8g',\n          'ËúÇËúú': '20g'\n        }\n      }, {\n        id: 6,\n        name: 'ÂÖ®È∫¶ËîìË∂äËéìÈù¢ÂåÖ',\n        price: 15.99,\n        image: 'https://via.placeholder.com/80',\n        categoryId: 'whole-wheat',\n        description: 'ÈÖ∏ÁîúÂèØÂè£ÁöÑÂÖ®È∫¶ËîìË∂äËéìÈù¢ÂåÖ',\n        ingredients: {\n          'ÂÖ®È∫¶Èù¢Á≤â': '350g',\n          'È´òÁ≠ãÈù¢Á≤â': '150g',\n          'ËîìË∂äËéìÂπ≤': '80g',\n          'Ê∞¥': '300ml',\n          'ÈÖµÊØç': '5g',\n          'Áõê': '8g',\n          'Á≥ñ': '20g'\n        }\n      }, {\n        id: 7,\n        name: 'ËäùÈ∫ªÈù¢ÂåÖ',\n        price: 13.99,\n        image: 'https://via.placeholder.com/80',\n        categoryId: 'specialty',\n        description: 'È¶ôËÑÜÂèØÂè£ÁöÑËäùÈ∫ªÈù¢ÂåÖ',\n        ingredients: {\n          'È´òÁ≠ãÈù¢Á≤â': '400g',\n          'ÈªëËäùÈ∫ª': '50g',\n          'ÁôΩËäùÈ∫ª': '50g',\n          'Ê∞¥': '250ml',\n          'ÈÖµÊØç': '5g',\n          'Áõê': '8g',\n          'Á≥ñ': '20g'\n        }\n      }, {\n        id: 8,\n        name: 'Ê†∏Ê°ÉÈù¢ÂåÖ',\n        price: 16.99,\n        image: 'https://via.placeholder.com/80',\n        categoryId: 'specialty',\n        description: 'È¶ôËÑÜÂèØÂè£ÁöÑÊ†∏Ê°ÉÈù¢ÂåÖ',\n        ingredients: {\n          'È´òÁ≠ãÈù¢Á≤â': '400g',\n          'Ê†∏Ê°É‰ªÅ': '100g',\n          'Ê∞¥': '250ml',\n          'ÈÖµÊØç': '5g',\n          'Áõê': '8g',\n          'Á≥ñ': '20g'\n        }\n      }, {\n        id: 9,\n        name: 'ÈªëÈ∫¶Èù¢ÂåÖ',\n        price: 14.99,\n        image: 'https://via.placeholder.com/80',\n        categoryId: 'specialty',\n        description: 'ÂÅ•Â∫∑Ëê•ÂÖªÁöÑÈªëÈ∫¶Èù¢ÂåÖ',\n        ingredients: {\n          'ÈªëÈ∫¶Èù¢Á≤â': '300g',\n          'È´òÁ≠ãÈù¢Á≤â': '200g',\n          'Ê∞¥': '300ml',\n          'ÈÖµÊØç': '5g',\n          'Áõê': '8g',\n          'Á≥ñ': '15g'\n        }\n      }, {\n        id: 10,\n        name: 'Ëë°ËêÑÂπ≤Èù¢ÂåÖ',\n        price: 14.99,\n        image: 'https://via.placeholder.com/80',\n        categoryId: 'sweet',\n        description: 'È¶ôÁîúÂèØÂè£ÁöÑËë°ËêÑÂπ≤Èù¢ÂåÖ',\n        ingredients: {\n          'È´òÁ≠ãÈù¢Á≤â': '400g',\n          'Ê∞¥': '250ml',\n          'ÈÖµÊØç': '5g',\n          'Áõê': '8g',\n          'Á≥ñ': '30g',\n          'Ëë°ËêÑÂπ≤': '100g',\n          'ÈªÑÊ≤π': '30g'\n        }\n      }, {\n        id: 11,\n        name: 'Â•∂Ê≤πÈù¢ÂåÖ',\n        price: 13.99,\n        image: 'https://via.placeholder.com/80',\n        categoryId: 'sweet',\n        description: 'ÊùæËΩØÈ¶ôÁîúÁöÑÂ•∂Ê≤πÈù¢ÂåÖ',\n        ingredients: {\n          'È´òÁ≠ãÈù¢Á≤â': '400g',\n          'ÁâõÂ•∂': '200ml',\n          'ÈÖµÊØç': '5g',\n          'Áõê': '8g',\n          'Á≥ñ': '40g',\n          'ÈªÑÊ≤π': '50g',\n          'È∏°Ëõã': '1‰∏™'\n        }\n      }, {\n        id: 12,\n        name: 'Â∑ßÂÖãÂäõÈù¢ÂåÖ',\n        price: 15.99,\n        image: 'https://via.placeholder.com/80',\n        categoryId: 'sweet',\n        description: 'ÊµìÈÉÅÈ¶ôÁîúÁöÑÂ∑ßÂÖãÂäõÈù¢ÂåÖ',\n        ingredients: {\n          'È´òÁ≠ãÈù¢Á≤â': '400g',\n          'ÁâõÂ•∂': '200ml',\n          'ÈÖµÊØç': '5g',\n          'Áõê': '8g',\n          'Á≥ñ': '40g',\n          'ÂèØÂèØÁ≤â': '30g',\n          'Â∑ßÂÖãÂäõË±Ü': '100g'\n        }\n      }]\n    };\n  },\n  computed: {\n    filteredBread() {\n      return this.breadData.filter(bread => bread.name.toLowerCase().includes(this.searchTerm.toLowerCase()) && (this.selectedCategory === 'all' || bread.categoryId === this.selectedCategory));\n    },\n    filteredCategories() {\n      if (this.selectedCategory === 'all') {\n        return this.categories.filter(category => this.breadData.some(bread => bread.categoryId === category.id && bread.name.toLowerCase().includes(this.searchTerm.toLowerCase())));\n      } else {\n        return this.categories.filter(category => category.id === this.selectedCategory && this.breadData.some(bread => bread.categoryId === category.id && bread.name.toLowerCase().includes(this.searchTerm.toLowerCase())));\n      }\n    },\n    totalFilteredBread() {\n      return this.filteredBread.length;\n    }\n  },\n  methods: {\n    getBreadByCategory(categoryId) {\n      return this.breadData.filter(bread => bread.categoryId === categoryId && bread.name.toLowerCase().includes(this.searchTerm.toLowerCase()));\n    },\n    editBread(bread) {\n      this.editingBread = {\n        ...bread\n      };\n      this.ingredientNames = Object.keys(bread.ingredients);\n      this.ingredientAmounts = Object.values(bread.ingredients);\n      this.showEditModal = true;\n    },\n    deleteBread(bread) {\n      this.breadToDelete = bread;\n      this.showDeleteModal = true;\n    },\n    closeEditModal() {\n      this.showEditModal = false;\n    },\n    closeDeleteModal() {\n      this.showDeleteModal = false;\n    },\n    saveBreadInfo() {\n      // Implementation of saveBreadInfo method\n    },\n    removeIngredient(index) {\n      // Implementation of removeIngredient method\n    },\n    addIngredient() {\n      // Implementation of addIngredient method\n    },\n    confirmDelete() {\n      // Implementation of confirmDelete method\n    }\n  }\n};","map":{"version":3,"names":["name","data","searchTerm","view","selectedCategory","showEditModal","showDeleteModal","editingBread","breadToDelete","ingredientNames","ingredientAmounts","categories","id","breadData","price","image","categoryId","description","ingredients","computed","filteredBread","filter","bread","toLowerCase","includes","filteredCategories","category","some","totalFilteredBread","length","methods","getBreadByCategory","editBread","Object","keys","values","deleteBread","closeEditModal","closeDeleteModal","saveBreadInfo","removeIngredient","index","addIngredient","confirmDelete"],"sources":["src/components/BreadInfo.vue"],"sourcesContent":["<template>\r\n  <div class=\"bread-info\">\r\n    <h1 class=\"page-title\">Èù¢ÂåÖ‰ø°ÊÅØÁÆ°ÁêÜ</h1>\r\n    \r\n    <!-- ÊêúÁ¥¢ÂíåËßÜÂõæÂàáÊç¢ -->\r\n    <div class=\"control-panel\">\r\n      <div class=\"search-container\">\r\n        <input \r\n          type=\"search\" \r\n          v-model=\"searchTerm\" \r\n          placeholder=\"ÊêúÁ¥¢Èù¢ÂåÖ...\" \r\n          class=\"search-input\"\r\n        >\r\n        <i class=\"search-icon\">üîç</i>\r\n      </div>\r\n      \r\n      <div class=\"view-controls\">\r\n        <button \r\n          :class=\"['view-btn', { active: view === 'grid' }]\" \r\n          @click=\"view = 'grid'\"\r\n        >\r\n          ÁΩëÊ†ºËßÜÂõæ\r\n        </button>\r\n        <button \r\n          :class=\"['view-btn', { active: view === 'table' }]\" \r\n          @click=\"view = 'table'\"\r\n        >\r\n          Ë°®Ê†ºËßÜÂõæ\r\n        </button>\r\n      </div>\r\n    </div>\r\n    \r\n    <!-- ÂàÜÁ±ªÈÄâÊã©Âô® -->\r\n    <div class=\"category-selector\">\r\n      <button \r\n        :class=\"['category-btn', { active: selectedCategory === 'all' }]\" \r\n        @click=\"selectedCategory = 'all'\"\r\n      >\r\n        ÂÖ®ÈÉ®ÂàÜÁ±ª\r\n      </button>\r\n      <button \r\n        v-for=\"category in categories\" \r\n        :key=\"category.id\"\r\n        :class=\"['category-btn', { active: selectedCategory === category.id }]\" \r\n        @click=\"selectedCategory = category.id\"\r\n      >\r\n        {{ category.name }}\r\n      </button>\r\n    </div>\r\n    \r\n    <!-- ÁΩëÊ†ºËßÜÂõæ -->\r\n    <div v-if=\"view === 'grid'\" class=\"grid-view\">\r\n      <div v-for=\"category in filteredCategories\" :key=\"category.id\" class=\"category-section\">\r\n        <h2 class=\"category-title\">{{ category.name }}</h2>\r\n        <div class=\"bread-grid\">\r\n          <div \r\n            v-for=\"bread in getBreadByCategory(category.id)\" \r\n            :key=\"bread.id\" \r\n            class=\"bread-card\"\r\n          >\r\n            <div class=\"bread-card-header\">\r\n              <h3 class=\"bread-name\">{{ bread.name }}</h3>\r\n              <span class=\"price-tag\">¬•{{ bread.price.toFixed(2) }}</span>\r\n            </div>\r\n            <div class=\"bread-card-content\">\r\n              <div class=\"bread-image-container\">\r\n                <img :src=\"bread.image\" :alt=\"bread.name\" class=\"bread-image\">\r\n              </div>\r\n              <div class=\"bread-details\">\r\n                <div class=\"ingredients-list\">\r\n                  <h4>ÂéüÊñô:</h4>\r\n                  <ul>\r\n                    <li v-for=\"(amount, ingredient) in bread.ingredients\" :key=\"ingredient\">\r\n                      {{ ingredient }}: {{ amount }}\r\n                    </li>\r\n                  </ul>\r\n                </div>\r\n                <div class=\"bread-description\">\r\n                  {{ bread.description }}\r\n                </div>\r\n              </div>\r\n              <div class=\"action-buttons\">\r\n                <button class=\"edit-btn\" @click=\"editBread(bread)\">ÁºñËæë‰ø°ÊÅØ</button>\r\n                <button class=\"delete-btn\" @click=\"deleteBread(bread)\">Âà†Èô§</button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      \r\n      <!-- Êó†ÁªìÊûúÊèêÁ§∫ -->\r\n      <div v-if=\"filteredCategories.length === 0\" class=\"no-results\">\r\n        Ê≤°ÊúâÊâæÂà∞ÂåπÈÖçÁöÑÈù¢ÂåÖ‰∫ßÂìÅ\r\n      </div>\r\n    </div>\r\n    \r\n    <!-- Ë°®Ê†ºËßÜÂõæ -->\r\n    <div v-else class=\"table-view\">\r\n      <div v-for=\"category in filteredCategories\" :key=\"category.id\" class=\"category-section\">\r\n        <h2 class=\"category-title\">{{ category.name }}</h2>\r\n        <table class=\"bread-table\">\r\n          <thead>\r\n            <tr>\r\n              <th class=\"image-col\">ÂõæÁâá</th>\r\n              <th>ÂêçÁß∞</th>\r\n              <th>‰ª∑Ê†º</th>\r\n              <th>ÂéüÊñô</th>\r\n              <th>ÊèèËø∞</th>\r\n              <th class=\"action-col\">Êìç‰Ωú</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"bread in getBreadByCategory(category.id)\" :key=\"bread.id\">\r\n              <td>\r\n                <img :src=\"bread.image\" :alt=\"bread.name\" class=\"table-bread-image\">\r\n              </td>\r\n              <td class=\"bread-name\">{{ bread.name }}</td>\r\n              <td>¬•{{ bread.price.toFixed(2) }}</td>\r\n              <td>\r\n                <div class=\"ingredients-cell\">\r\n                  <span v-for=\"(amount, ingredient) in bread.ingredients\" :key=\"ingredient\">\r\n                    {{ ingredient }}: {{ amount }}\r\n                  </span>\r\n                </div>\r\n              </td>\r\n              <td class=\"description-cell\">{{ bread.description }}</td>\r\n              <td class=\"action-col\">\r\n                <button class=\"table-action-btn edit\" @click=\"editBread(bread)\">ÁºñËæë</button>\r\n                <button class=\"table-action-btn delete\" @click=\"deleteBread(bread)\">Âà†Èô§</button>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n      \r\n      <!-- Êó†ÁªìÊûúÊèêÁ§∫ -->\r\n      <div v-if=\"filteredCategories.length === 0\" class=\"no-results\">\r\n        Ê≤°ÊúâÊâæÂà∞ÂåπÈÖçÁöÑÈù¢ÂåÖ‰∫ßÂìÅ\r\n      </div>\r\n    </div>\r\n    \r\n    <!-- ÂàÜÈ°µ -->\r\n    <div class=\"pagination\">\r\n      <div class=\"pagination-info\">\r\n        ÊòæÁ§∫ {{ totalFilteredBread }} ‰∏™Èù¢ÂåÖ‰∏≠ÁöÑ {{ totalFilteredBread }} ‰∏™\r\n      </div>\r\n      <div class=\"pagination-controls\">\r\n        <button class=\"pagination-btn\" disabled>\r\n          &lt;\r\n        </button>\r\n        <span class=\"pagination-text\">Á¨¨ 1 È°µÔºåÂÖ± 1 È°µ</span>\r\n        <button class=\"pagination-btn\" disabled>\r\n          &gt;\r\n        </button>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- ÁºñËæëÈù¢ÂåÖ‰ø°ÊÅØÊ®°ÊÄÅÊ°Ü -->\r\n    <div v-if=\"showEditModal\" class=\"modal\">\r\n      <div class=\"modal-content\">\r\n        <div class=\"modal-header\">\r\n          <h2>ÁºñËæëÈù¢ÂåÖ‰ø°ÊÅØ</h2>\r\n          <button class=\"close-btn\" @click=\"closeEditModal\">&times;</button>\r\n        </div>\r\n        <div class=\"modal-body\">\r\n          <form @submit.prevent=\"saveBreadInfo\" class=\"bread-form\">\r\n            <div class=\"form-group\">\r\n              <label>ÂêçÁß∞</label>\r\n              <input type=\"text\" v-model=\"editingBread.name\" required>\r\n            </div>\r\n            <div class=\"form-group\">\r\n              <label>‰ª∑Ê†º</label>\r\n              <input type=\"number\" v-model=\"editingBread.price\" step=\"0.01\" required>\r\n            </div>\r\n            <div class=\"form-group\">\r\n              <label>ÂàÜÁ±ª</label>\r\n              <select v-model=\"editingBread.categoryId\" required>\r\n                <option v-for=\"category in categories\" :key=\"category.id\" :value=\"category.id\">\r\n                  {{ category.name }}\r\n                </option>\r\n              </select>\r\n            </div>\r\n            <div class=\"form-group\">\r\n              <label>ÊèèËø∞</label>\r\n              <textarea v-model=\"editingBread.description\" rows=\"3\" required></textarea>\r\n            </div>\r\n            <div class=\"form-group\">\r\n              <label>ÂéüÊñô</label>\r\n              <div v-for=\"(amount, ingredient, index) in editingBread.ingredients\" :key=\"index\" class=\"ingredient-input\">\r\n                <input type=\"text\" v-model=\"ingredientNames[index]\" placeholder=\"ÂéüÊñôÂêçÁß∞\" required>\r\n                <input type=\"text\" v-model=\"ingredientAmounts[index]\" placeholder=\"Áî®Èáè\" required>\r\n                <button type=\"button\" class=\"remove-ingredient\" @click=\"removeIngredient(index)\">√ó</button>\r\n              </div>\r\n              <button type=\"button\" class=\"add-ingredient\" @click=\"addIngredient\">Ê∑ªÂä†ÂéüÊñô</button>\r\n            </div>\r\n            <div class=\"form-actions\">\r\n              <button type=\"button\" class=\"cancel-btn\" @click=\"closeEditModal\">ÂèñÊ∂à</button>\r\n              <button type=\"submit\" class=\"save-btn\">‰øùÂ≠ò</button>\r\n            </div>\r\n          </form>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Âà†Èô§Á°ÆËÆ§Ê®°ÊÄÅÊ°Ü -->\r\n    <div v-if=\"showDeleteModal\" class=\"modal\">\r\n      <div class=\"modal-content delete-confirm\">\r\n        <h3>Á°ÆËÆ§Âà†Èô§</h3>\r\n        <p>Á°ÆÂÆöË¶ÅÂà†Èô§Èù¢ÂåÖ \"{{ breadToDelete?.name }}\" ÂêóÔºüÊ≠§Êìç‰Ωú‰∏çÂèØÊÅ¢Â§ç„ÄÇ</p>\r\n        <div class=\"modal-actions\">\r\n          <button class=\"cancel-btn\" @click=\"closeDeleteModal\">ÂèñÊ∂à</button>\r\n          <button class=\"delete-btn\" @click=\"confirmDelete\">Á°ÆËÆ§Âà†Èô§</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'BreadInfo',\r\n  data() {\r\n    return {\r\n      searchTerm: '',\r\n      view: 'grid',\r\n      selectedCategory: 'all',\r\n      showEditModal: false,\r\n      showDeleteModal: false,\r\n      editingBread: null,\r\n      breadToDelete: null,\r\n      ingredientNames: [],\r\n      ingredientAmounts: [],\r\n      categories: [\r\n        { id